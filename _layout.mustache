<script src="https://cdn.jsdelivr.net/npm/prismjs@1.8.1/prism.min.js"></script>

<!--
    Select the current nav link.
-->
<script>
    var elements = document.getElementsByClassName('nav-link');
    var loc = window.location.pathname;
    for (var i = 0; i < elements.length; i++){
        if (elements[i].getAttribute('href') == loc){
            elements[i].className += ' active';
        }
    }
</script>

</body>
</html>